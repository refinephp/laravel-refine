{
    "name": "refinephp/laravel-refine",
    "description": "A Laravel package for elegant and efficient query filtering and sorting.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Refinephp\\LaravelRefine\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ricardo Di Zio",
            "email": "ricardodizio@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^11.0|^10.0|^9.0|^8.0|^12.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Refinephp\\LaravelRefine\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^8.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.5.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Refinephp\\LaravelRefine\\LaravelRefineServiceProvider"
            ]
        }
    }
}
